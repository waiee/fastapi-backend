{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/main.py",
            "methods": [
                "GET",
                "POST",
                "PUT", 
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        }
    ],
    "env": {"APP_MODULE": "main:app"}
}